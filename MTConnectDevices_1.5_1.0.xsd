  <xs:element name='LocalReference' type='LocalReferenceType' substitutionGroup='Reference'>
    <xs:annotation>
      <xs:documentation>
        A reference to a data item or component
      </xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name='DataItemRefType'>
    <xs:annotation>
      <xs:documentation>
        dataitemref XML element is a pointer to a data entity associated with
        another structural element defined elsewhere in the XML document for a
        piece of equipment.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base='LocalReferenceType'/>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name='DataItemRef' type='DataItemRefType' substitutionGroup='LocalReference'>
    <xs:annotation>
      <xs:documentation>
        dataitemref XML element is a pointer to a data entity associated with
        another structural element defined elsewhere in the XML document for a
        piece of equipment.
      </xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name='ComponentRefType'>
    <xs:annotation>
      <xs:documentation>
        componentref XML element is a pointer to all of the information
        associated with another structural element defined elsewhere in the XML
        document for a piece of equipment. 
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base='LocalReferenceType'>
        <xs:attribute name='relation' type='RelationshipType' use='optional'>
          <xs:annotation>
            <xs:documentation>
              The relationship to the other item
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name='ComponentRef' type='ComponentRefType' substitutionGroup='LocalReference'>
    <xs:annotation>
      <xs:documentation>
        componentref XML element is a pointer to all of the information
        associated with another structural element defined elsewhere in the XML
        document for a piece of equipment. 
      </xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name='DeviceRefType'>
    <xs:annotation>
      <xs:documentation>
        A data item reference
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base='ReferenceType'>
        <xs:attribute name='deviceUuid' type='DeviceUuidType' use='required'>
          <xs:annotation>
            <xs:documentation>
              The piece of equipments UUID that supplied this data. This is an
              optional element references to the UUID attribute given in the
              device element. This can be any series of numbers and letters as
              defined by the XML type nmtoken.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name='relation' type='RelationshipType' use='optional'>
          <xs:annotation>
            <xs:documentation>
              The relationship to the other item
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name='href' type='xlink:hrefType' use='optional'>
          <xs:annotation>
            <xs:documentation>
              Reference to the url of the related device
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute ref='xlink:type' use='optional' fixed='locator'>
          <xs:annotation>
            <xs:documentation>
              Type of href fixed at located
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name='DeviceRef' type='DeviceRefType' substitutionGroup='Reference'>
    <xs:annotation>
      <xs:documentation>
        A data item reference
      </xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:simpleType name='SampleIntervalType'>
    <xs:annotation>
      <xs:documentation>
        The interval between adjacent sampleing of data
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base='xs:float'/>
  </xs:simpleType>
  <xs:simpleType name='DataItemSampleRateType'>
    <xs:annotation>
      <xs:documentation>
        The frequency a measurement is sampled
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base='xs:float'/>
  </xs:simpleType>
  <xs:simpleType name='DataItemOptionType'>
    <xs:annotation>
      <xs:documentation>
        The constrained value for this data item
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base='xs:string'/>
  </xs:simpleType>
  <xs:simpleType name='DataItemValueType'>
    <xs:annotation>
      <xs:documentation>
        The constrained value for this data item
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base='xs:string'/>
  </xs:simpleType>
  <xs:simpleType name='DataItemNumericValueType'>
    <xs:annotation>
      <xs:documentation>
        The constrained value for this data item
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base='xs:float'/>
  </xs:simpleType>
  <xs:simpleType name='SourceComponentIdType'>
    <xs:annotation>
      <xs:documentation>
        An idref to the component id
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base='xs:IDREF'/>
  </xs:simpleType>
  <xs:simpleType name='SourceDataItemIdType'>
    <xs:annotation>
      <xs:documentation>
        An idref to the data item id
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base='xs:IDREF'/>
  </xs:simpleType>
  <xs:simpleType name='ComponentNameType'>
    <xs:annotation>
      <xs:documentation>
        The name of a related component
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base='xs:NMTOKEN'/>
  </xs:simpleType>
  <xs:simpleType name='FilterValueType'>
    <xs:annotation>
      <xs:documentation>
        The minimum limit on the change in a value
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base='xs:float'/>
  </xs:simpleType>
  <xs:simpleType name='CategoryType'>
    <xs:annotation>
      <xs:documentation>
        Specifies the kind of information provided by a data item. 
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base='xs:string'>
      <xs:enumeration value='EVENT'>
        <xs:annotation>
          <xs:documentation>
            An event represents a change in state occurs at a point in time.
            Note: An event does not occur at predefined frequencies.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value='SAMPLE'>
        <xs:annotation>
          <xs:documentation>
            A sample is a data point for continuous data items, that is, the
            value of a data item at a point in time.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value='CONDITION'>
        <xs:annotation>
          <xs:documentation>
            The condition of the device
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name='NativeScaleType'>
    <xs:annotation>
      <xs:documentation>
        The multiplier for the native value. Conversion divides by this value
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base='xs:float'/>
  </xs:simpleType>
  <xs:simpleType name='RepresentationType'>
    <xs:annotation>
      <xs:documentation>
        Description of a means to interpret data consisting of multiple data
        points or samples reported as a single value. representation is an
        optional attribute. representation will define a unique format for each
        set of data. representation for timeseries representation, discrete
        representation, and value representation are defined below in Section
        red 7.2.2.12. If representation is not specified, it MUST be determined
        to be value representation.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base='xs:string'>
      <xs:enumeration value='DISCRETE'>
        <xs:annotation>
          <xs:documentation>
            A data entity where each discrete occurrence of the data may have
            the same value as the previous occurrence of the data.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value='TIME_SERIES'>
        <xs:annotation>
          <xs:documentation>
            A series of sampled data. 
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
      <xs:enumeration value='VALUE'>
        <xs:annotation>
          <xs:documentation>
            The measured value of the sample data.
          </xs:documentation>
        </xs:annotation>
      </xs:enumeration>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name='DataItemFilterEnumType'>
    <xs:annotation>
      <xs:documentation>
        The type of filter
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base='xs:string'>
      <xs:enumeration value='MINIMUM_DELTA'/>
      <xs:enumeration value='PERIOD'/>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name='DataItemsType'>
    <xs:annotation>
      <xs:documentation>
        XML container consisting of one or more types of dataitem XML elements.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name='DataItem' type='DataItemType' minOccurs='1' maxOccurs='unbounded'>
        <xs:annotation>
          <xs:documentation>
            data entity describing a piece of information reported about a piece
            of equipment.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name='DataItemType'>
    <xs:annotation>
      <xs:documentation>
        data entity describing a piece of information reported about a piece of
        equipment.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name='Source' type='DataItemSourceType' minOccurs='0' maxOccurs='1'>
        <xs:annotation>
          <xs:documentation>
            Additional information about the component, channel, register,
            etc... that collects the data.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name='Constraints' type='DataItemConstraintsType' minOccurs='0' maxOccurs='1'>
        <xs:annotation>
          <xs:documentation>
            constraints is an optional container that provides a set of expected
            values that can be reported for this dataitem.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name='Filters' type='FiltersType' minOccurs='0' maxOccurs='1'>
        <xs:annotation>
          <xs:documentation>
            An XML container consisting of one or more types of filter XML
            elements.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name='InitialValue' type='DataItemNumericValueType' minOccurs='0' maxOccurs='1'>
        <xs:annotation>
          <xs:documentation>
            initialvalue is an optional XML element that defines the starting
            value for a data item as well as the value to be set for the data
            item after a reset event.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name='ResetTrigger' type='DataItemResetValueType' minOccurs='0' maxOccurs='1'>
        <xs:annotation>
          <xs:documentation>
            resettrigger is an optional XML element that identifies the type of
            event that may cause a reset to occur. It is additional information
            regarding the meaning of the data that establishes an understanding
            of the time frame that the data represents so that the data may be
            correctly understood by a client software application.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
    <xs:attribute name='name' type='NameType' use='optional'>
      <xs:annotation>
        <xs:documentation>
          The optional implementors name of the measurement.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='id' type='IDType' use='required'>
      <xs:annotation>
        <xs:documentation>
          The unique identifier for this element.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='type' type='DataItemEnumType' use='required'>
      <xs:annotation>
        <xs:documentation>
          The type of measurement
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='subType' type='DataItemSubEnumType' use='optional'>
      <xs:annotation>
        <xs:documentation>
          The sub type for the measurement
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='statistic' type='DataItemStatisticsType' use='optional'>
      <xs:annotation>
        <xs:documentation>
          The statistical operation on this data
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='units' type='UnitsType' use='optional'>
      <xs:annotation>
        <xs:documentation>
          The units of the measurement
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='nativeUnits' type='NativeUnitsType' use='optional'>
      <xs:annotation>
        <xs:documentation>
          The units as expressed by the machine
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='nativeScale' type='NativeScaleType' use='optional'>
      <xs:annotation>
        <xs:documentation>
          The units as expressed by the machine
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='category' type='CategoryType' use='required'>
      <xs:annotation>
        <xs:documentation>
          Specifies the kind of information provided by a data item. 
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='coordinateSystem' type='CoordinateSystemType' use='optional'>
      <xs:annotation>
        <xs:documentation>
          For measured values relative to a coordinate system like position
          sample, the coordinate system being used may be reported.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='compositionId' type='CompositionIdType' use='optional'>
      <xs:annotation>
        <xs:documentation>
          The identifier attribute of the composition element that the reported
          data is most closely associated. 
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='sampleRate' type='DataItemSampleRateType' use='optional'>
      <xs:annotation>
        <xs:documentation>
          Used as the default sample rate for waveforms
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='representation' type='RepresentationType' use='optional' default='VALUE'>
      <xs:annotation>
        <xs:documentation>
          Description of a means to interpret data consisting of multiple data
          points or samples reported as a single value. representation is an
          optional attribute. representation will define a unique format for
          each set of data. representation for timeseries representation,
          discrete representation, and value representation are defined below in
          Section red 7.2.2.12. If representation is not specified, it MUST be
          determined to be value representation.
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
    <xs:attribute name='significantDigits' type='SignificantDigitsValueType' use='optional'>
      <xs:annotation>
        <xs:documentation>
          The number of significant digits for this data item
        </xs:documentation>
      </xs:annotation>
    </xs:attribute>
  </xs:complexType>
  <xs:complexType name='DataItemConstraintsType'>
    <xs:annotation>
      <xs:documentation>
        A set of limits for a data item
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:choice minOccurs='0' maxOccurs='1'>
        <xs:sequence minOccurs='1'>
          <xs:element name='Value' type='DataItemValueElementType' minOccurs='1' maxOccurs='unbounded'>
            <xs:annotation>
              <xs:documentation>
                value represents a single data value that is expected to be
                reported for a dataitem element. 
              </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
        <xs:sequence minOccurs='1'>
          <xs:element name='Minimum' type='DataItemNumericValueType' minOccurs='0' maxOccurs='1'>
            <xs:annotation>
              <xs:documentation>
                The lower limit of data reported for a data item.
              </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name='Maximum' type='DataItemNumericValueType' minOccurs='0' maxOccurs='1'>
            <xs:annotation>
              <xs:documentation>
                The upper limit of data reported for a data item.
              </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name='Nominal' type='DataItemNumericValueType' minOccurs='0' maxOccurs='1'>
            <xs:annotation>
              <xs:documentation>
                The target or expected value for this data item.
              </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:choice>
      <xs:element name='Filter' type='DataItemFilterType' minOccurs='0' maxOccurs='1'>
        <xs:annotation>
          <xs:documentation>
            filter provides a means to control when an agent records updated
            information for a data item.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name='FiltersType'>
    <xs:annotation>
      <xs:documentation>
        An XML container consisting of one or more types of filter XML elements.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name='Filter' type='DataItemFilterType' minOccurs='1' maxOccurs='unbounded'>
        <xs:annotation>
          <xs:documentation>
            filter provides a means to control when an agent records updated
            information for a data item.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name='DataItemBaseType'>
    <xs:annotation>
      <xs:documentation>
        A starting point for a data item
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base='DataItemValueType'/>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name='DataItemValueElementType'>
    <xs:annotation>
      <xs:documentation>
        The value element
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base='DataItemOptionType'/>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name='DataItemFilterType'>
    <xs:annotation>
      <xs:documentation>
        The filter for the data item
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base='FilterValueType'>
        <xs:attribute name='type' type='DataItemFilterEnumType' use='required'>
          <xs:annotation>
            <xs:documentation>
              The type of filter, ABSOLUTE or PERCENT
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name='DataItemSourceType'>
    <xs:annotation>
      <xs:documentation>
        A native data source
      </xs:documentation>
    </xs:annotation>
    <xs:simpleContent>
      <xs:extension base='ItemSourceType'>
        <xs:attribute name='dataItemId' type='SourceDataItemIdType' use='optional'>
          <xs:annotation>
            <xs:documentation>
              The identifier attribute of the dataitem that represents the
              originally measured value of the data referenced by this data
              item.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name='componentId' type='SourceComponentIdType' use='optional'>
          <xs:annotation>
            <xs:documentation>
              The identifier attribute of the component element that represents
              the physical part of a piece of equipment where the data
              represented by the dataitem element originated.
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute name='compositionId' type='CompositionIdType' use='optional'>
          <xs:annotation>
            <xs:documentation>
              The identifier attribute of the composition element that the
              reported data is most closely associated. 
            </xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
</xs:schema>