<xs:schema xmlns:xs='http://www.w3.org/2001/XMLSchema' xmlns='urn:mtconnect.org:MTConnectAssets:1.5' xmlns:mt='urn:mtconnect.org:MTConnectAssets:1.5' targetNamespace='urn:mtconnect.org:MTConnectAssets:1.5' elementFormDefault='qualified' attributeFormDefault='unqualified' xmlns:xlink='http://www.w3.org/1999/xlink'>
